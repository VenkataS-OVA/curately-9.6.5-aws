	"scripts": {
		"dev": "vite",
		"start": "set NODE_ENV=development & vite",
		"start-qa": "set NODE_ENV=development & vite --mode qa",
		"start-prod": "set NODE_ENV=development & vite --mode prod",
		"start-devqa": "set NODE_ENV=development & vite --mode devqa",
		"build": "set NODE_ENV=production & vite build --mode prod",
		"build-qa": "set NODE_ENV=production & vite build --mode qa",
		"build-compress": "vite build && compress-cra -c /compress-cra.json",
		"build-ts-compress": "tsc && vite build && compress-cra -c /compress-cra.json",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"analyze": "npx vite-bundle-visualizer --mode -m development",
		"analyze1": "build --stats && webpack-bundle-analyzer build/bundle-stats.json",
		"deploy": "aws s3 sync curately/ s3://curatelyai-9.6.5",
		"prod": "npm run build && npm run deploy"
	},
